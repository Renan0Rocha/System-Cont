<Page x:Class="System_Cont.Views.PagInicialFormPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:System_Cont.Views"
      mc:Ignorable="d" 
      d:DesignHeight="965" d:DesignWidth="1920"
      Title="PagInicialFormPage">

    <Grid Background="#1E1E1E">
        <Grid.RowDefinitions>
            <RowDefinition Height="550"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0">
            <Label Content="Página Inicial" FontWeight="Normal" FontSize="50" Foreground="#DDFFFFFF"/>
            <Rectangle Width="400" Height="200" Fill="#FF0A0D10" HorizontalAlignment="Left" Margin="300 150 0 0"/>
        </StackPanel>
        
    </Grid>
</Page>
